<?xml version="1.0" encoding="UTF-8" ?>
<!-- Paths are relative from current working directory (CWD) where phpdoc is executed -->
<phpdoc>
    <title>Template v3.6.0 Documentation</title>

    <parser>
        <target>./pdocs/cache</target>

        <!-- Read files with theses extensions -->
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>

    <transformer>
        <target>./pdocs</target>
    </transformer>

    <files>
        <directory>./src</directory>
    </files>
</phpdoc>